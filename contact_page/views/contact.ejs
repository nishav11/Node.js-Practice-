        <form class="contact-form" method="post" action="/contact">

            <label for="who">Whom do you want to contact:</label>
            <input type="text" class="form-control" name="who" value="<%= qs.name %>">

            <label for="department">Which Department:</label>
            <input type="text" class="form-control" name="department" value="<%= qs.dept %>">


            <label for="email"> Your Email:</label>
            <input type="email" class="form-control" name="email">

            <button type="submit" class="btn btn-default">Submit</button>
        </form>
</body>

